    "build:platform": "npm run create_cordova_folder && npm run check-del-cli && del-cli --force \"platforms\" && cordova platform rm android && cordova platform add android@12.0.0",
    "build:release": "ionic cordova build android --prod --release",
    "build:bundle": " cd platforms/android && gradle bundle && cd..",
    "check": "--------------------------------------------------",
    "check-unzipper": "npm list unzipper || npm install unzipper",
    "check-del-cli": "npm list del-cli || npm install del-cli",
    "delete": "----------------------------------------------------------------------------------------------------------------------------------",
    "delete:files": "cd playstore && del-cli --force \"app-release.aab\" && del-cli --force  \"emf.aab\" && del-cli --force \"universal.apk\" && del-cli --force \"emf.apk\"",
    "delete:zips": "cd playstore && del-cli --force \"my_app.zip\" && del-cli --force \"my_app\"",
    "copy": "---------------------------------------------------------------------------------------------------------",
    "copy:bundle": "copy \"platforms\\android\\app\\build\\outputs\\bundle\\release\\app-release.aab\"  \"playstore.\"",
    "sign": "--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------",
    "sign:bundle": "cd playstore && \"C:\\PROGRA~1\\Java\\jdk-11.0.16\\bin\\jarsigner\" -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore \"emf.keystore\" -storepass \"vayapedal\"   app-release.aab  \"vayapedal\"  && \"C:\\Users\\Diego\\AppData\\Local\\Android\\Sdk\\build-tools\\30.0.3\\zipalign\" -v 4 app-release.aab  \"emf.aab\" && cd ..",
    "unpack": "cd playstore && java -jar bundletool-all-1.14.0.jar build-apks --bundle=app-release.aab --output=my_app.apks --mode=universal  --ks=\"emf.keystore\"  --ks-key-alias=\"vayapedal\"  --ks-pass=pass:\"vayapedal\"  && cd ..",
    "zips": "------------------------------------------------------------------------------------------------------------------------------------",
    "rename": "cd playstore && node -e \"require('fs').rename('my_app.apks', 'my_app.zip', function(err) { if (err) console.log(err);})\" && cd..",
    "rename:apk": "cd playstore && node -e \"const name = require('./../package.json').apkfile; require('fs').rename('universal.apk', name, function(err) { if (err) console.log(err);})\"  && cd..",
    "copy:apk": "copy \"playstore\\my_app\\universal.apk\" \"playstore.\"",
    "unzip": "cd playstore && node -e \"require('fs').createReadStream('./my_app.zip').pipe(require('unzipper').Extract({ path: './my_app' }));\" && cd..",
    "": "-----------------------------------------------------------------------------------------------------------",
    "install:BC": "ionic cordova plugins add \"E:/PROYECTOS/_AVA projects/plugins_cordova/barcode-scan\"",
    "debug": "node dev-version.js && ionic cordova run android",
    "build:debug": "npm run build:platform && npm run debug",
    "build:prod": "npm run print:platform && npm run build:platform && npm run print:release && npm run build:release && npm run print:bundle && npm run build:bundle && npm run check-del-cli && npm run delete:files && npm run copy:bundle && npm run print:sign && npm run sign:bundle  && npm run print:unzip &&  npm run check-unzipper && npm run unpack && npm run rename && npm run unzip && npm run copy:apk && npm run rename:apk && npm run delete:zips && npm run print:apkdir && npm run open:apkdir"
  },
